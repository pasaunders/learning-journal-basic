{% extends "layout.jinja2" %}
{% block title %}title_overrides_default{% endblock %}

{% block body %}
<h2>Journal Entries</h2>
<ul>
    {% for item in entry_list %}
        <li><a href="{{request.route_url('detail', id=item.id) }}">{{ item.title }}</a></li>
        <li>{{ item.creation_date}}
    {% endfor %}
</ul>
{% endblock %}
